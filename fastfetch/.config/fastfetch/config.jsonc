{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "display": {
        "size": {
            "maxPrefix": "GB",
            "ndigits": 1
        }
    },
    "modules": [
        "title",
        "separator",
        "os",
//        "host",
        {
            "type": "kernel",
            "format": "{release}"
        },
        "uptime",
        "packages",
//        "shell",
        {
            "type": "display",
            "compactType": "original",
            "key": "Resolution"
        },
        "de",
//        "wm",
//        "wmtheme",
        "theme",
        "icons",
        "terminal",
        {
            "type": "terminalfont",
            "format": "{/name}{-}{/}{name}{?size} {size}{?}"
        },
        "cpu",
        {
            "type": "gpu",
            "key": "GPU"
        },
        {
            "type": "memory",
	    "format": "{} / {} ({percentage})"
        },
        {
            "type": "battery",
            "temp": true
        },
        "break",
        "colors"
    ]
}
